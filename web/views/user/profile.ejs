<h1>User Page</h1>
<ul>
  <li>User ID : <%= id %></li>
  <li>User firstName : <%= firstName %></li>
</ul>
<h2>Service list</h2>
<ul>
	<% webftps.forEach(function(item){ %>
    	<li><%= item.name %></li>
    	<li><%= item.ports %></li>
    	<li><%= item.ftpuser %></li>
    	<li><%= item.mysqlUser %></li>
	<% } %>
</ul>
<ul>
	<% vpsusers.forEach(function(item){ %>
    	<li><%= item.name %></li>
    	<li><%= item.ports %></li>
    	<li><%= item.user %></li>
	<% } %>
</ul>
<ul>
	<% vpsroot.forEach(function(item){ %>
    	<li><%= item.name %></li>
    	<li><%= item.ports %></li>
    	<li><%= item.user %></li>
	<% } %>
</ul>
<a href="/create" class="button big ">Ajouter un service</a>